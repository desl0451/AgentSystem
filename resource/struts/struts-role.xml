<?xml version="1.0" encoding="UTF-8" ?>  
    <!DOCTYPE struts PUBLIC   
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"   
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="role" namespace="/" extends="global">
		<default-action-ref name="pagenotfound" />
		
		<action name="rolelist" class="roleAction" method="roleList">
			<result>/WEB-INF/pages/rolelist.jsp</result>
		</action>
		
		<action name="editrole" class="roleAction" method="editRole"></action>
		<action name="deleterole" class="roleAction" method="deleteRole"></action>
	</package>
</struts>  
